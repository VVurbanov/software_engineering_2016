	{
	if(b!=0)
		{
		var x;
		x=-c/b;	
		print={
		"x":x					
		};
		console.log(JSON.stringify(print,"\n",1));
	}
else if(b==0 && c==0 )
	{
		console.log("All integers are 0.Therefore there are no roots.");
		return 1;		
	}
else
	{
	var x1,x2;
	var D;
	D=b*b - 4*a*c;
	x1=(-b+Math.sqrt(D))/2/a;
	x1=(-b-Math.sqrt(D))/2/a;
	print={
	"x1":x1,
	"x2":x2,
	"D":D
	console.log(JSON.stringify(print,"\n",1));
}
	
	
